 author = {Sudhakar, Goparaju Purna},
 year = {2012},
 title = {A Model of Critical Success Factors for Software Projects},
 url = {10.1108/17410391211272829},
 keywords = {CRITICAL success factor;RESEARCH;COMPUTER software;PROJECT management;BUSINESS information services;CONCEPTUAL models;Critical success factors;Information systems;Managing software projects;project success;Team factors},
